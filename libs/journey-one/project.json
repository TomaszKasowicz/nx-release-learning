{
  "name": "journey-one",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/journey-one/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": ["scope:journey", "type:shell"],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/journey-one/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/journey-one/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/journey-one/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/journey-one/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
